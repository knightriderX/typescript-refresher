{
  "compilerOptions": {
    "experimentalDecorators": true,
    "lib": [
      "ES6",
      "DOM"
    ],
    "target": "es6",
    "rootDir": "src",
    "outDir": "dist",
    "baseUrl": "src",
    "paths": {
      "@components/*": [
        "data/components/*"
      ]
    },
    "noImplicitAny": true,
    "noImplicitThis": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "strict": true
  }
}